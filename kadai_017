class Human:
    def __init__(self, name, age):
        self.name = name
        self.age = age
    
    def check_adult(self):
        if(self.age >= 20):
            print(f"{self.name}さんは大人です．")
        else:
            print(f"{self.name}さんは大人ではないです．")
        

akiko = Human("あきこ", 16)
hitomi = Human("ひとみ", 24)
yuki = Human("ゆうき", 14)
wataru = Human("わたる", 35)

humans =[akiko, hitomi, yuki, wataru]

for human in humans:
    human.check_adult()
